             xmlns:local="clr-namespace:QuickHomeExpenseSummarizer"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <Style x:Key="DefaultLabelStyle" TargetType="Label">
                <Setter Property="Foreground" Value="White" />
            </Style>

        </ResourceDictionary>
    </Application.Resources>
</Application>
